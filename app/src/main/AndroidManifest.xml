<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.truepower">

    <application
        android:allowBackup="true"
        android:fontFamily="@style/"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TruePower">

        <activity
            android:name=".EditMeal"
            android:exported="true"
            android:label="Edit Meal"/>
        <activity
            android:name=".ViewMeal"
            android:exported="true"
            android:label="View Meal"/>
        <activity
            android:name=".AddMeal"
            android:exported="true"
            android:label="Add Meal"/>
        <activity
            android:name=".EditRoutine"
            android:exported="true"
            android:label="My Routine" />
        <activity
            android:name=".StartActivity"
            android:exported="true"
            android:label="Start Here" />
        <activity
            android:name=".ViewRoutine"
            android:exported="true"
            android:label="View Routine" />
        <activity
            android:name=".AddRoutine"
            android:exported="true"
            android:label="My Routine" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>